<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="android.example.bunew.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/courses"
        android:name="android.example.bunew.ui.courses.GalleryFragment"
        android:label="@string/courses"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/enroll"
        android:name="android.example.bunew.ui.enroll.SlideshowFragment"
        android:label="Enrollment"
        tools:layout="@layout/fragment_slideshow" />


    <fragment
        android:id="@+id/login"
        android:name="android.example.bunew.fragment.Login"
        android:label="Log In"
        tools:layout="@layout/fragment_login" />

    <fragment
        android:id="@+id/notice"
        android:name="android.example.bunew.fragment.Notice"
        android:label="Notice"
        tools:layout="@layout/fragment_notice" />

    <fragment
        android:id="@+id/news"
        android:name="android.example.bunew.fragment.News"
        android:label="News &amp; Events"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/result"
        android:name="android.example.bunew.fragment.Result"
        android:label="Result"
        tools:layout="@layout/fragment_result" />

    <fragment
        android:id="@+id/degree"
        android:name="android.example.bunew.fragment.degree"
        android:label="Degree"
        tools:layout="@layout/fragment_degree" />


</navigation>